table#tblAlumnos(style="border:1px solid green")
    thead
        tr
            th no control 
            th Nombre 
            th Carrera
    tbody 
        each alumnos in [{nc:"12", alumno: "Cande", carrera: "ISC"},{nc:"20", alumno: "Cande2", carrera: "ISC"},{nc:"25", alumno: "Cande3", carrera: "ISC"}]
            tr
                td #{alumnos.nc}
                td #{alumnos.alumno}
                td #{alumnos.carrera}